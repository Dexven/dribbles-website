{
    "name": "dribbles.info",
    "type": "silverstripe-recipe",
    "description": "Dribbles Website Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "4.2",
        "silverstripe/userforms": "^5.1",
        "silverstripe/spamprotection": "3.0.0",
        "undefinedoffset/silverstripe-nocaptcha": "dev-master",
        "silverstripe/blog": "^3.1",
        "dnadesign/silverstripe-elemental": "2.1.2",
        "dnadesign/silverstripe-elemental-userforms": "1.0.0",
        "dnadesign/silverstripe-elemental-list": "1.0.0",
        "dynamic/silverstripe-elemental-blog": "1.0.0",
        "dynamic/silverstripe-elemental-accordion": "dev-master",
        "dynamic/silverstripe-elemental-countdown": "dev-master",
        "dynamic/silverstripe-elemental-flexslider": "dev-master",
        "undefinedoffset/sortablegridfield": "2.0.6",
        "jonom/silverstripe-betternavigator": "4.1.1",
        "symbiote/silverstripe-gridfieldextensions": "3.2.0",
        "heyday/silverstripe-colorpalette": "2.0.3",
        "silverstripe/comments": "3.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "mysite/_config/*",
            ".env.example"
        ],
        "expose": [
            "themes/dribbles/css",
            "themes/dribbles/sass",
            "themes/dribbles/assets"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "branch-alias": {
            "4.x-dev": "4.2.x-dev",
            "dev-master": "5.x-dev"
        },
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "platform": []
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
