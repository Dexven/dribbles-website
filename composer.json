{
    "name": "dribbles.info",
    "type": "silverstripe-recipe",
    "description": "Dribbles Website Installer",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Dexven/facebook-token-converter",
            "name": "dexven/facebook-token-converter"
        }
    ],
    "require": {
        "php": ">=5.6",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "4.4.1",
        "silverstripe/userforms": "^5.4.0",
        "silverstripe/spamprotection": "3.0.0",
        "undefinedoffset/silverstripe-nocaptcha": "^2",
        "silverstripe/blog": "^3.3.0",
        "dnadesign/silverstripe-elemental": "3.1.1",
        "dnadesign/silverstripe-elemental-userforms": "2.0.0",
        "dynamic/silverstripe-elemental-blog": "1.0.1",
        "dynamic/silverstripe-elemental-countdown": "1.0.0",
        "dynamic/silverstripe-elemental-flexslider": "1.0.2",
        "jonom/silverstripe-betternavigator": "4.1.1",
        "symbiote/silverstripe-gridfieldextensions": "3.2.1",
        "heyday/silverstripe-colorpalette": "dev-master",
        "silverstripe/comments": "3.1.5",
        "dexven/facebook-token-converter": "dev-master",
        "unclecheese/display-logic": "2.0.2",
        "gorriecoe/silverstripe-link": "1.2.4",
        "gorriecoe/silverstripe-linkfield": "dev-master",
        "jonom/focuspoint": "3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "classmap": [
            "app/src/Page.php",
            "app/src/PageController.php"
        ]
    },
    "extra": {
        "project-files": [
            "app/_config/*",
            ".env.example"
        ],
        "expose": [
            "themes/dribbles"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "branch-alias": {
            "4.x-dev": "4.3.x-dev",
            "dev-master": "5.x-dev"
        },
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
